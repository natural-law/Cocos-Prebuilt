{
    "copy_config" : [
        {
            "from": "cocos2d-x/build",
            "to": "gen/cocos/frameworks/cocos2d-x/build",
            "include": [
                "cocos2d_libs.xcodeproj"
            ]
        },
        {
            "from": "cocos2d-x/cocos",
            "to": "gen/cocos/frameworks/cocos2d-x/cocos",
            "exclude": [
                "*linux/*",
                "*wp8*",
                "*winrt*",
                "*CMakeLists.txt",
                "*build/*"
            ]
        },
        {
            "from": "cocos2d-x/extensions",
            "to": "gen/cocos/frameworks/cocos2d-x/extensions",
            "exclude": [
                "*linux/*",
                "*wp8*",
                "*CMakeLists.txt"
            ]
        },
        {
            "from": "cocos2d-x/external",
            "to": "gen/cocos/frameworks/cocos2d-x/external",
            "exclude": [
                "*linux/*",
                "*wp8*",
                "*winrt*",
                "*CMakeLists.txt",
                "lua/lua/*.c",
                "lua/luajit/*.sh",
                "lua/luajit/src/*"
            ]
        },
        {
            "from": "cocos2d-x/templates",
            "to": "gen/cocos/frameworks/cocos2d-x/templates",
            "exclude": [
                "*CMakeLists.txt",
                "lua-template-default/*",
                "*linux/*",
                "*wp8*"
            ]
        },
        {
            "from": "console-wrapper",
            "to": "gen/cocos/frameworks/cocos2d-x/tools/cocos2d-console",
            "exclude": [
                "*.pyc"
            ]
        },
        {
            "from": "cocos2d-x/tools/cocos2d-console",
            "to": "gen/cocos/frameworks/cocos2d-console",
            "exclude": [
                "*.pyc",
                "download-bin.py",
                "config.json",
                "version.json"
            ]
        },
        {
            "from": "x-templates",
            "to": "gen/cocos/frameworks/cocos2d-x/templates",
            "include": [
                "cocos2dx_files.json",
                "cpp-template-default/cocos-project-template.json"
            ]
        },
        {
            "from": "cocos2d-x/plugin",
            "to": "gen/cocos/frameworks/cocos2d-x/plugin"
        }
    ],
    "android_mks" : [
    ],
    "xcode_proj_info" : {
        "cocos2d-x/build/cocos2d_libs.xcodeproj" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-x/prebuilt",
            "targets" :[ "build all libs" ]
        },
        "cocos2d-x/cocos/scripting/lua-bindings/proj.ios_mac/cocos2d_lua_bindings.xcodeproj" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-x/prebuilt",
            "targets" : [ "luabindings" ]
        }
    },
    "win32_proj_info" : {
        "cocos2d-x/build/cocos2d-win32.vc2012.sln" : {
            "outputdir" : "gen/cocos/frameworks/cocos2d-x/prebuilt/win32",
            "targets" : [
                "libAudio", "libBox2D", "libchipmunk",
                "libcocos2d", "libCocosBuilder", "libCocosStudio",
                "libExtensions", "libGUI", "libLocalStorage",
                "liblua", "libNetwork", "libSpine"
            ]
        }
    }
}
